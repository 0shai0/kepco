<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.DemoMapper">

<mapper namespace="com.example.board.mapper.DemoMapper">
    <select id="select" resultType="java.until.Map">
        select * from demo
    </select>
    <select id="selectById" resultType="java.util.Map">
        select * from demo where seq = ${seq}
    </select>
</mapper>